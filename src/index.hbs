{{#extend "layout" title="COVID-19 Tracker India"}}

    {{#replace "sidebar"}}
        {{> sidebar this conditionClass=conditionClass active="index"}}
    {{/replace}}

    {{#replace "app_content"}}
        <h2 class="page-title">COVID-19 TRACKER <small>Crowdsourced Initiative By InverseSoft</small></h2>
        <div class="row">
            <div class="col-lg-12">
                <section class="widget">
                    <header>
                        <h4>
                            <img src="img/live.gif" alt="livecovid19.org" height="15"/> LIVE INDIA
                            <small>
                                Trends.
                            </small>
                        </h4>
                        <div class="widget-controls">
                            <a title="Options" href="#"><i class="glyphicon glyphicon-cog"></i></a>
                            <a data-widgster="expand" title="Expand" href="#"><i class="glyphicon glyphicon-chevron-up"></i></a>
                            <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-chevron-down"></i></a>
                            <a data-widgster="close" title="Close" href="#"><i class="glyphicon glyphicon-remove"></i></a>
                        </div>
                    </header>
                    <div class="body no-margin">
                        <div id="visits-chart" class="chart visits-chart">
                            <svg></svg>
                        </div>
                        <div class="visits-info well well-sm">
                            <div class="row">
                                <div class="col-sm-3 col-xs-6">
                                    <div class="key"><i class="fa fa-users"></i> Confirmed</div>
                                    <div class="value"><span id="Confirmed">0</span> <i class="fa fa-caret-up color-red"></i> <span id="ConfirmedToday" class="changeData"></span></div>
                                </div>
                                <div class="col-sm-3 col-xs-6">
                                    <div class="key"><i class="fa fa-bed"></i> Active</div>
                                    <div class="value"><span id="Active">0</span> </div>
                                </div>
                                <div class="col-sm-3 col-xs-6">
                                    <div class="key"><i class="fa fa-plus-square"></i> Recovered</div>
                                    <div class="value"><span id="Recovered">0</span> <i class="fa fa-caret-up color-green"></i> <span id="RecoveredToday" class="changeData"></span></div>
                                </div>
                                <div class="col-sm-3 col-xs-6">
                                    <div class="key"><i class="fa fa-ban"></i> Deceased</div>
                                    <div class="value"><span id="Deceased">0</span> <i class="fa fa-caret-up color-red"></i> <span id="DeceasedToday" class="changeData"></span></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <section class="widget">
                    <header>
                        <h4>
                            By States
                            <small>
                                &amp; Districts
                            </small>
                        </h4>
                        <div class="widget-controls">
                            <a data-widgster="expand" title="Expand" href="#"><i class="glyphicon glyphicon-plus"></i></a>
                            <a data-widgster="collapse" title="Collapse" href="#"><i class="glyphicon glyphicon-minus"></i></a>
                            <a data-widgster="close" title="Close" href="#"><i class="glyphicon glyphicon-remove"></i></a>
                        </div>
                    </header>
                    <div class="table-responsive">
                        <table id="StateData" class="table table-striped table-lg mt-sm mb-0 sources-table">
                            <thead>
                            <tr class="child">
                                <th class="expander"></th>
                                <th class="source-col-header">state</th>
                                <th>confirmed</th>
                                <th>active</th>
                                <th>deaths</th>
                                <th>recovered</th>
                                <th>updated</th>
                            </tr>
                            </thead>
                            <tbody>
                            
                            </tbody>
                        </table>
                    </div>
                </section>
